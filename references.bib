@ARTICLE{Zohm1996,
  author = {H Zohm},
  title = {Edge localized modes (ELMs)},
  journal = {Plasma Physics and Controlled Fusion},
  year = {1996},
  volume = {38},
  pages = {105},
  number = {2},
  abstract = {The phenomenology of edge localized modes (ELMs), an MHD instability
	occurring in the edge of H-mode plasmas in toroidal magnetic fusion
	experiments, is described. ELMs are important to obtain experimental
	control of the particle inventory of fusion plasmas. From an analysis
	of the ELM behaviour of different magnetic fusion experiments, three
	distinct types are identified, namely dithering cycles, type III
	and type I ELMs. A physical picture of these phenomena is established
	on the grounds of theoretical models put forward to describe the
	different ELM phenomena.},
  file = {Zohm1996.pdf:Zohm1996.pdf:PDF},
  owner = {jrwalk},
  timestamp = {2012.05.02},
  url = {http://stacks.iop.org/0741-3335/38/i=2/a=001}
